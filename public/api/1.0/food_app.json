{
  "apiVersion": "1.0",
  "swaggerVersion": "1.2",
  "basePath": "http://localhost:3000",
  "resourcePath": "food_app",
  "apis": [
    {
      "path": "/sign_in.json",
      "operations": [
        {
          "summary": "Register user in the system",
          "parameters": [
            {
              "paramType": "form",
              "name": "email",
              "type": "string",
              "description": "Email",
              "required": true
            },
            {
              "paramType": "form",
              "name": "password",
              "type": "string",
              "description": "Password",
              "required": true
            },
            {
              "paramType": "form",
              "name": "restaurant_id",
              "type": "string",
              "description": "restaurant id",
              "required": true
            },
            {
              "paramType": "form",
              "name": "pos_name",
              "type": "string",
              "description": "Pos system name",
              "required": true,
              "allowableValues": {
                "valueType": "LIST",
                "values": [
                  "iiko"
                ]
              }
            },
            {
              "paramType": "form",
              "name": "pos_url",
              "type": "string",
              "description": "Pos system url",
              "required": false
            },
            {
              "paramType": "form",
              "name": "pos_login",
              "type": "string",
              "description": "Pos system login",
              "required": false
            },
            {
              "paramType": "form",
              "name": "pos_password",
              "type": "string",
              "description": "Pos system pass",
              "required": false
            },
            {
              "paramType": "form",
              "name": "sync_data",
              "type": "boolean",
              "description": "syncronization required",
              "required": false
            }
          ],
          "responseMessages": [
            {
              "code": 200,
              "responseModel": null,
              "message": "Ok"
            },
            {
              "code": 401,
              "responseModel": null,
              "message": "Unauthorized"
            }
          ],
          "nickname": "FoodApp#sign_in",
          "method": "post"
        }
      ]
    }
  ],
  "authorizations": null
}